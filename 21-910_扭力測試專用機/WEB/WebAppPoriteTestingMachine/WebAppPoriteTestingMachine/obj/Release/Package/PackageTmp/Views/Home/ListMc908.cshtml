@model IEnumerable<WebAppPoriteTestingMachine.Models.MC908Table>

@{
    Layout = null;
    int DataCount = Model.Count();
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListMc908</title>
</head>
<body>
    @*<p>
            @Html.ActionLink("Create New", "Create")
        </p>*@
    <table class="table" border="1">
        <tr>
            <th>
                <p>條碼</p>
            </th>
            <th>
                <p>測試日期時間</p>
            </th>
            <th>
                <p>正轉判斷結果</p>
            </th>
            <th>
               <p>反轉判斷結果</p>
            </th>
            <th>
                <p>正轉峰值轉速</p>
            </th>
            <th>
                <p>反轉峰值轉速</p>
            </th>
            <th>
               <p>正轉峰值電流</p>
            </th>
            <th>
                <p>反轉峰值電流</p>
            </th>
            <th>
               <p>正轉平均轉速</p>
            </th>
            <th>
                <p>反轉平均轉速</p>
            </th>
            <th>
                <p>正轉平均電流</p>
            </th>
            <th>
                <p>反轉平均電流</p>
            </th>
        </tr>

        @foreach (var item in Model)
        {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.QRCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TestingDateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CCWJudgmentResult)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CWJudgmentResult)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CCWSpeed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CWSpeed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CCWCurrent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CWCurrent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CCWAvgSpeed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CWAvgSpeed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CCWAvgCurrent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CWAvgCurrent)
        </td>
    </tr>
        }

    </table>
    <hr />
    <p>共計 @Html.Label(DataCount.ToString())筆資料</p>
</body>
</html>
